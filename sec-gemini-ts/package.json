{
  "name": "sec-gemini",
  "version": "1.1.1",
  "description": "Sec-Gemini SDK",
  "license": "Apache-2.0",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist demo/*.js demo/*.map",
    "build": "tsup",
    "test": "jest",
    "demo": "tsup && node demo/demo.js",
    "relink": "npm run clean && tsup && npm rm secgeminisdk && npm link",
    "prepublishOnly": "npm run build",
    "format-verify": "prettier ./src/* ./tests/* --check",
    "format": "prettier ./src/* ./tests/* --write"
  },
  "dependencies": {
    "@hey-api/client-fetch": "^0.7.0",
    "debug": "^4.4.0",
    "isomorphic-ws": "^5.0.0",
    "prettier": "^3.5.3",
    "stable": "^0.1.8"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "^0.62.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.5",
    "jest": "^29.7.0",
    "openapi-typescript": "^7.4.4",
    "ts-jest": "^29.2.5",
    "tsup": "^8.4.0",
    "typescript": "^5.7.2"
  }
}
